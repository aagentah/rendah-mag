{
  "name": "rendah-mag",
  "version": "2.5.1",
  "description": "Est. 2018 - Creative UK-based outlet, primarily focused on exposing the progressive and innovative side to underground bass music.",
  "main": "index.js",
  "engines": {
    "node": "8.x",
    "npm": "5.x"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "beats",
    "halftime",
    "drum & bass",
    "music",
    "magaazine"
  ],
  "author": "Dan Jones (Aagentah)",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "scripts": {
    "start": "better-npm-run start",
    "heroku-postbuild": "npm run build",
    "start:heroku": "npm run start:prod",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "better-npm-run start:prod",
    "start:prodlocal": "npm run build && better-npm-run start:prodlocal",
    "build": "npm run clean:build && better-npm-run build",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "better-npm-run lint:js",
    "lint:style": "better-npm-run lint:style",
    "flow": "better-npm-run flow",
    "clean:all": "npm run clean:build && npm run clean:test",
    "clean:build": "better-npm-run clean:build",
    "clean:test": "better-npm-run clean:test",
    "coveralls": "better-npm-run coveralls && npm run clean:test"
  },
  "betterScripts": {
    "start": {
      "command": "nodemon ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8080
      }
    },
    "start:prod": {
      "command": "PORT=${PORT:-8080} node ./index.js",
      "env": {
        "NODE_HOST": "0.0.0.0",
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "BROWSER": "none"
      }
    },
    "start:prodlocal": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "build": {
      "command": "webpack --progress --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "lint:js": {
      "command": "eslint ./src ./tools ./index.js"
    },
    "lint:style": {
      "command": "stylelint \"./src/**/*.scss\" --syntax scss"
    },
    "flow": {
      "command": "flow; test $? -eq 0 -o $? -eq 2"
    },
    "clean:build": {
      "command": "rimraf ./public/assets"
    },
    "clean:test": {
      "command": "rimraf ./coverage"
    },
    "coveralls": {
      "command": "cat ./coverage/lcov.info | coveralls"
    }
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-remove-console"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "react/prop-types": 0,
      "linebreak-style": 0,
      "global-require": 0,
      "no-underscore-dangle": 0,
      "no-console": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "function-paren-newline": 0
    },
    "globals": {
      "__CLIENT__": true,
      "__SERVER__": true,
      "__DISABLE_SSR__": true,
      "__DEV__": true,
      "webpackIsomorphicTools": true
    }
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@sanity/block-content-to-react": "^2.0.6",
    "@sanity/client": "^0.140.8",
    "axios": "^0.18.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.3.0",
    "classnames": "^2.2.5",
    "compression": "1.7.1",
    "connected-react-router": "^6.6.1",
    "cors": "^2.8.4",
    "express": "4.17.1",
    "express-lowercase-paths": "^1.0.0",
    "helmet": "3.21.2",
    "history": "^4.10.1",
    "hpp": "^0.2.2",
    "lodash": "^4.17.11",
    "morgan": ">=1.9.1",
    "npm": "^6.9.0",
    "opn": "^5.1.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-facebook": "^5.0.3",
    "react-facebook-pixel": "^0.1.3",
    "react-frontload": "1.1.0",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "4.12.10",
    "react-iframe": "^1.8.0",
    "react-instafeed": "^0.4.0",
    "react-lazyload": "^2.3.0",
    "react-mailchimp-subscribe": "^2.0.2",
    "react-messenger-customer-chat": "^0.7.1",
    "react-progressive-bg-image": "^3.0.0",
    "react-progressive-image": "^0.6.0",
    "react-progressive-image-loading": "^3.0.3",
    "react-redux": "^7.1.3",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "react-soundcloud-player": "^1.0.1",
    "react-spotify-player": "^1.0.4",
    "react-youtube": "^7.5.0",
    "redbox-react": "^1.5.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.2.0",
    "rendah-pattern-library": "github:danjonesdev/rendah-pattern-library",
    "serialize-javascript": "^1.4.0",
    "serve-favicon": "2.4.5",
    "styled-components": "^4.3.1",
    "webpack-isomorphic-tools": "^3.0.5",
    "xmlbuilder": "^13.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "better-npm-run": "^0.1.0",
    "coveralls": "3.0.12",
    "cross-spawn": "^6.0.5",
    "css-loader": "^0.28.7",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "flow-bin": "0.57.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^5.0.0",
    "nock": "10.0.6",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.7",
    "postcss": "^7.0.17",
    "postcss-loader": "^3.0.0",
    "raf": "^3.4.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "16.0.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^1.1.2",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-sources": "1.4.1"
  }
}
